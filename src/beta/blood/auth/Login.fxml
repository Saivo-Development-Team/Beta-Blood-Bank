        <Label alignment="CENTER" layoutX="11.0" layoutY="31.0" prefHeight="86.0" prefWidth="113.0" text="Welcome to Beta Blood" textAlignment="CENTER" textFill="#fc0000" wrapText="true">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <ImageView fitHeight="150.0" fitWidth="113.0" layoutX="11.0" layoutY="221.0" onMouseClicked="#openWebpage" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../../../resources/sanbs.jpg" />
            </image>
        </ImageView>
    </children>
</AnchorPane>
