<AnchorPane id="AnchorPane" accessibleText="loginchoice" prefHeight="400.0" prefWidth="600.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="beta.blood.auth.LoginController">
    <children>
        <Label layoutX="160.0" layoutY="118.0" prefHeight="17.0" prefWidth="140.0" text="Employee Number" textFill="#f80000">
            <font>
                <Font size="13.0" />
            </font>
        </Label>
        <Label layoutX="160.0" layoutY="163.0" prefHeight="17.0" prefWidth="140.0" text="Password" textFill="#f20000">
            <font>
                <Font size="13.0" />
            </font>
        </Label>
        <Button fx:id="login_btn" layoutX="300.0" layoutY="221.0" mnemonicParsing="false" onAction="#login" prefHeight="25.0" prefWidth="84.0" text="Login" />
        <TextField fx:id="number" layoutX="300.0" layoutY="114.0" />
        <PasswordField fx:id="password" layoutX="300.0" layoutY="159.0" />
        <Button layoutX="414.0" layoutY="221.0" mnemonicParsing="false" onAction="#exit" text="Exit" />
        <Button layoutX="545.0" layoutY="14.0" mnemonicParsing="false" onAction="#help" text="Help" />
